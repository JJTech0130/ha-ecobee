{
  "version": "0.1.0",
  "domain": "ecobee",
  "name": "ecobee",
  "codeowners": [],
  "config_flow": true,
  "documentation": "https://www.home-assistant.io/integrations/ecobee",
  "homekit": {
    "models": ["EB", "ecobee*"]
  },
  "iot_class": "cloud_polling",
  "loggers": ["pyecobee", "custom_components.ecobee"],
  "requirements": [],
  "zeroconf": [
    {
      "type": "_ecobee._tcp.local."
    },
    {
      "type": "_sideplay._tcp.local.",
      "properties": {
        "mdl": "eb-*"
      }
    },
    {
      "type": "_sideplay._tcp.local.",
      "properties": {
        "mdl": "ecobee*"
      }
    }
  ]
}
